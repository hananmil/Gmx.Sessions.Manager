{
    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "Microsoft": "Warning",
            "System": "Warning"
        }
    },
    "SessionTimeoutMinutes": 10,
    "ServerUpReporter": {
        "ReportIntervalSeconds": 5
    },
    "LocalSessionRepository": {
        "Path": "/tmp/sessions"
    },
    "Redis": {
        "ConnectionString": "redis:6379"
    },
    "ClusterPrefix": "cluster1",
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://+:5000"
            }
        }
    }
}